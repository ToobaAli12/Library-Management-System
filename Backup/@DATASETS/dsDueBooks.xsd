<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsDueBooks" targetNamespace="http://tempuri.org/dsDueBooks.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/dsDueBooks.xsd" xmlns:mstns="http://tempuri.org/dsDueBooks.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsDueBooks" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="tblBarrowed">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="BarrowerID" type="xs:string" minOccurs="0" />
							<xs:element name="Fullname" type="xs:string" minOccurs="0" />
							<xs:element name="BookNo" type="xs:string" minOccurs="0" />
							<xs:element name="ISBN" type="xs:string" minOccurs="0" />
							<xs:element name="BookTitle" type="xs:string" minOccurs="0" />
							<xs:element name="CategoryName" type="xs:string" minOccurs="0" />
							<xs:element name="DateBarrowed" type="xs:date" minOccurs="0" />
							<xs:element name="DateDue" type="xs:date" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>